{
    "type": "package",
    "name": "ericnething/elm-xmpp",
    "summary": "An XMPP client library for Elm",
    "license": "Apache-2.0",
    "version": "1.0.0",
    "exposed-modules": [
        "XMPP",
        "XMPP.Connection",
        "XMPP.JID",
        "XMPP.Stanza",
        "XMPP.Stanza.Message",
        "XMPP.Stanza.Presence",
        "XMPP.Stanza.Iq"
    ],
    "elm-version": "0.19.0 <= v < 0.20.0",
    "dependencies": {
        "elm/core": "1.0.0 <= v < 2.0.0",
        "elm/parser": "1.1.0 <= v < 2.0.0",
        "jinjor/elm-xml-parser": "2.0.0 <= v < 3.0.0",
        "ymtszw/elm-xml-decode": "3.1.0 <= v < 4.0.0"
    },
    "test-dependencies": {
        "elm-explorations/test": "1.1.0 <= v < 2.0.0"
    }
}
