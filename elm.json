{
    "type": "package",
    "name": "ericnething/elm-xmpp",
    "summary": "An XMPP client library for Elm",
    "license": "Apache-2.0",
    "version": "1.0.0",
    "exposed-modules": {
        "Basics": [
            "Xmpp",
            "Xmpp.Connection",
            "Xmpp.JID"
        ],
        "Stanza": [
            "Xmpp.Stanza",
            "Xmpp.Stanza.Message",
            "Xmpp.Stanza.Presence",
            "Xmpp.Stanza.Iq"
        ],
        "Xml": [
            "Xmpp.Xml",
            "Xmpp.Xml.Decode"
        ]
    },
    "elm-version": "0.19.0 <= v < 0.20.0",
    "dependencies": {
        "elm/core": "1.0.0 <= v < 2.0.0",
        "elm/parser": "1.1.0 <= v < 2.0.0",
        "rtfeldman/elm-hex": "1.0.0 <= v < 2.0.0",
        "truqu/elm-base64": "2.0.4 <= v < 3.0.0"
    },
    "test-dependencies": {
        "elm-explorations/test": "1.1.0 <= v < 2.0.0"
    }
}